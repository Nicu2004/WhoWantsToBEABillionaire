<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.RadialGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="333.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.wowantstobeamillionare.controllers.guest.GuestInterfaceController">
   <children>
      <Text fx:id="welcomeGuest" stroke="#9c1a1a" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" wrappingWidth="600.0">
         <font>
            <Font size="45.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
      </Text>
      <Button fx:id="playButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="0.0" prefWidth="147.0" text="StartTrial" textAlignment="CENTER" onAction="#playTrialGame">
         <font>
            <Font size="28.0" />
         </font>
         <textFill>
            <RadialGradient centerX="0.48672565527721845" centerY="0.5719697186441133" cycleMethod="REFLECT" focusDistance="-0.14678893832985418" radius="0.7201834949878377">
               <stops>
                  <Stop>
                     <color>
                        <Color />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color red="1.0" green="1.0" blue="1.0" />
                     </color>
                  </Stop>
               </stops>
            </RadialGradient>
         </textFill>
      </Button>
   </children>
</VBox>
